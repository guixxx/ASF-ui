		"before-build-webpack": "^0.2.9",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-vue": "^5.2.3",
		"file-loader": "^4.2.0",
		"node-sass": "^4.12.0",
		"sass-loader": "^7.3.1",
